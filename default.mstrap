[general]
arch=armhf
noauth=true
unpack=true
cleanup=true
omitrequired=true
markauto=true
bootstrap=ptux debian
aptsources=
aptpreferences=/usr/share/mkos-overostorm/preferences

[ptux]
source=http://packages.pragmatux.org/device
suite=3.0
components=main
packages={{device-package}}

[debian]
source=http://ftp.us.debian.org/debian
suite=wheezy
